<h1>Formulario de apadrinamientos Koricancha</h1>
<section class="insert">
  <form action="/insert" method="post">

    <div class="row">
      <div class="input box_inline">
        <label for="zone marginBottom">Zona</label>
        <input class="normalField" type="text" name="zone">
      </div>
      <div class="input box_inline">
        <label for="dateDoc marginBottom">Fecha <span class="spanlabel">(dd/mm/yyyy)</span></label>
        <input class="normalField" type="text" name="dateDoc">
      </div>
    </div>

    <h1>1.Datos personales</h1> 
    <div class="row">
      <div class="input box_inline">
        <label for="userName ">Nombre del usuario</label>
        <input class="normalField" type="text" name="userName">
      </div>
      <div class="input box_inline">
        <label for="firstLastName ">Primer apellido</label>
        <input class="normalField" type="text" name="firstLastName">
      </div>
      <div class="input box_inline">
        <label for="secondLastName ">Segundo apellido</label>
        <input class="normalField" type="text" name="secondLastName">
      </div>
      <div class="input box_inline">
        <label for="BdayUser ">Fecha de nacimiento</label>
        <input class="normalField" type="text" name="BdayUser">
      </div>

      <div class="input box_inline">
        <label for="dniUser ">DNI</label>
        <input class="normalField" type="text" name="dniUser">
      </div>
    </div>
    <div class="row">
      <div class="input box_inline">
        <label for="responsibleName ">Nombre del responsable</label>
        <input class="normalField" type="text" name="responsibleName">
      </div>
      <div class="input box_inline">
        <label for="relationship ">Parentesco</label>
        <input class="normalField" type="text" name="relationship">
      </div>  
      <div class="input box_inline">
        <label for="relationship ">DNI del responsable</label>
        <input class="normalField" type="text" name="responsibleDni">
      </div>  
      <div class="input box_inline">
        <label for="resposnibleTel ">Telefono del responsable</label>
        <input class="normalField" type="text" name="resposnibleTel">
      </div>  
    </div>

